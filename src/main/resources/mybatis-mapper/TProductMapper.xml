<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.transaction.dao.TProductDao">
    
    <insert id="addProduct" parameterType="com.example.transaction.entity.bo.ProductBo">
        INSERT INTO t_product (id, name, create_time)
        VALUES (#{id}, #{name}, #{createTime})
    </insert>

    <select id="getList" resultType="com.example.transaction.entity.TProduct">
        SELECT id,
               name,
               create_time as createTime
        FROM t_product
        ORDER BY create_time DESC
    </select>

    <select id="selectAll" resultType="com.example.transaction.entity.TProduct">
        SELECT * FROM t_product
    </select>

    <insert id="insert" parameterType="com.example.transaction.entity.TProduct">
        INSERT INTO t_product (id, name, create_time)
        VALUES (#{id}, #{name}, #{createTime})
    </insert>

</mapper> 